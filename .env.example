# Strapi API URL (publicly accessible)
NEXT_PUBLIC_STRAPI_URL=http://localhost:1337/api

# Strapi API Token (server-side only, for write operations)
# Generate this from your Strapi admin panel: Settings â†’ API Tokens
STRAPI_API_TOKEN=your_api_token_here

# Optional: Cache settings
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=100
